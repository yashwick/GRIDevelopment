@model IEnumerable<Product>
@{
    ViewData["Title"] = "Index";
}

<h1>product Index</h1>

<table class="table table-bordered table-striped" style="width:100%">
    <thead>
        <tr>
            <th>Product Name</th>
            @*<th>key</th>*@
        </tr>
    </thead>


    <tbody>
    @foreach (var item in Model)  
    {
        <tr>
            
            <td width="50%">
                @item.ProductName
            </td>
            @*<td>
                @item.ProductAttribute.Value
            </td>*@
        </tr>
        
    }
    </tbody>
</table>





